{
  "name": "PatternBP",
  "textClasses": {
    "label": { "fontSize": 12 },
    "callout": {
      "fontFace": "'Segoe UI', wf_segoe-ui_normal, helvetica, arial, sans-serif",
      "fontSize": 12
    },
    "title": {
      "fontFace": "'Segoe UI Semibold', wf_segoe-ui_semibold, helvetica, arial, sans-serif",
      "fontSize": 10
    },
    "header": { "fontSize": 10 }
  },
  "dataColors": [
    "#009101",
    "#f2f200",
    "#9ec338",
    "#0b334d",
    "#0057be",
    "#e9d224",
    "#e5ff2f",
    "#e84f30"
  ],

  "visualStyles": {
    "card": {
      "*": {
        "labels": [
          {
            "color": { "solid": { "color": "#000000" } },
            "labelDisplayUnits": "0",
            "labelPrecision": 0,
            "fontSize": 12,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryLabels": [
          {
            "show": false,
            "color": { "solid": { "color": "#000000" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "wordWrap": [
          {
            "show": true
          }
        ],
        "background": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "title": [
          {
            "show": true,
            "fontColor": { "solid": { "color": "#1A1A1A" } },
            "background": { "solid": { "color": "#E6E6E6" } },
            "alignment": "left",
            "fontSize": 11,
            "fontFamily": "Segoe UI"
          }
        ],

        "border": [
          {
            "show": false,
            "color": { "solid": { "color": "#FFFFFF" } },
            "radius": 5
          }
        ],
        "dropShadow": [
          {
            "show": true,
            "color": { "solid": { "color": "#000000" } },
            "position": "Outer",
            "preset": "Custom",
            "shadowSpread": 2,
            "shadowBlur": 1,
            "angle": 90,
            "shadowDistance": 1,
            "transparency": 70
          }
        ]
      }
    },
    "clusteredBarChart": {
      "*": {
        "general": [
          {
            "responsive": true
          }
        ],
        "legend": [
          {
            "show": false,
            "position": "Top",
            "showTitle": true,
            "titleText": "",
            "labelColor": { "solid": { "color": "#FFFF" } },
            "fontFamily": "Courier New",
            "fontSize": 10
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "position": "Right",
            "labelColor": { "solid": { "color": "#01B8AA" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI",
            "preferredCategoryWidth": 30,
            "maxMarginFactor": 28,
            "innerPadding": 22,
            "concatenateLabels": true,
            "showAxisTitle": false,
            "axisStyle": "showTitleOnly"
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "position": "Right",
            "axisScale": "Linear",
            "start": 0,
            "labelColor": { "solid": { "color": "#1a90f3" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "labelDisplayUnits": "0",
            "labelPrecision": "Auto",
            "showAxisTitle": false,
            "axisStyle": "showTitleOnly",
            "color": { "solid": { "color": "#1a90f3" } }
          }
        ],
        "dataPoint": [
          {
            "defaultColor": { "solid": { "color": "#0057be" } }
          }
        ],
        "labels": [
          {
            "show": true,
            "color": { "solid": { "color": "#000000" } },
            "labelDisplayUnits": "0",
            "labelPrecision": 1,
            "labelPosition": "OutsideEnd",
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "enableBackground": false,
            "backgroundColor": { "solid": { "color": "#0928DD" } },
            "backgroundTransparency": 30
          }
        ],
        "plotArea": [
          {
            "transparency": 20
          }
        ],
        "dropShadow": [
          {
            "show": true,
            "color": { "solid": { "color": "#000000" } },
            "position": "Outer",
            "preset": "Custom",
            "shadowSpread": 2,
            "shadowBlur": 1,
            "angle": 90,
            "shadowDistance": 1,
            "transparency": 70
          }
        ]
      }
    },

    "*": {
      "*": {
        "visualHeader": [
          {
            "background": {
              "solid": { "color": "#FFFFFF" },
              "transparency": 100
            }
          }
        ],
        "outspacePane": [
          { "backgroundColor": { "solid": { "color": "#FFFFFF" } } }
        ]
      }
    },

    "page": {
      "*": {
        "background": [
          { "color": { "solid": { "color": "#54D699" }, "transparency": 10 } }
        ],
        "outspace": [{ "color": { "solid": { "color": "#F5FFAC" } } }]
      }
    }
  }
}
